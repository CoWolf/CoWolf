<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.uni_stuttgart.iste.cowolf.ui.category.wizards.main"
            name="%category.name">
      </category>
      <category
            id="de.uni_stuttgart.iste.cowolf.ui.category.wizards.models"
            name="Models"
            parentCategory="de.uni_stuttgart.iste.cowolf.ui.category.wizards.main">
      </category>
      <wizard
            category="de.uni_stuttgart.iste.cowolf.ui.category.wizards.main"
            class="de.uni_stuttgart.iste.cowolf.ui.wizards.NewProjectWizard"
            finalPerspective="de.uni_stuttgart.iste.cowolf.ui.perspective"
            icon="icons/logo_wulf_15x15.png"
            id="de.uni_stuttgart.iste.cowolf.ui.wizard.new.custom"
            name="%wizard.name"
            project="true">
      </wizard>
      <wizard
            category="de.uni_stuttgart.iste.cowolf.ui.category.wizards.main"
            class="de.uni_stuttgart.iste.cowolf.ui.wizards.NewFolderWizard"
            icon="icons/logo_wulf_15x15.png"
            id="de.uni_stuttgart.iste.cowolf.ui.wizard.new.folder"
            name="%wizard.name.0">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="de.uni_stuttgart.iste.cowolf.ui.category.wizards"
            name="%category.name.0">
      </category>
      <wizard
            category="de.uni_stuttgart.iste.cowolf.ui.category.wizards"
            class="org.eclipse.ui.wizards.datatransfer.ExternalProjectImportWizard"
            icon="icons/logo_wulf_15x15.png"
            id="de.uni_stuttgart.iste.cowolf.ui.wizard.import"
            name="%wizard.name.1">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="de.uni_stuttgart.iste.cowolf.ui.category.wizards"
            name="%category.name.1">
      </category>
      <wizard
            category="de.uni_stuttgart.iste.cowolf.ui.category.wizards"
            class="org.eclipse.ui.wizards.datatransfer.ZipFileExportWizard"
            icon="icons/logo_wulf_15x15.png"
            id="de.uni_stuttgart.iste.cowolf.ui.wizard.export"
            name="%wizard.name.2">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.uni_stuttgart.iste.cowolf.ui.perspectives.Perspective"
            icon="icons/logo_wulf_15x15.png"
            id="de.uni_stuttgart.iste.cowolf.ui.perspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="de.uni_stuttgart.iste.cowolf.ui.perspective">
         <view
               id="de.uni_stuttgart.iste.cowolf.ui.navigator.view"
               minimized="false"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss">
         </view>
         <newWizardShortcut
               id="de.uni_stuttgart.iste.cowolf.ui.wizard.new.custom">
         </newWizardShortcut>
         <newWizardShortcut
               id="de.uni_stuttgart.iste.cowolf.ui.wizard.new.folder">
         </newWizardShortcut>
         <viewShortcut
               id="de.uni_stuttgart.iste.cowolf.ui.navigator.view">
         </viewShortcut>
         <view
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               ratio="0.75"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         id="de.uni_stuttgart.iste.cowolf.ui.projectNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="de.uni_stuttgart.false.cowolf.core.natures.ProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/C.png"
            id="de.uni_stuttgart.iste.cowolf.ui.natureImage"
            natureId="de.uni_stuttgart.iste.cowolf.ui.projectNature">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.uni_stuttgart.iste.cowolf.ui.preference.MainPreferencePage"
            id="de.uni_stuttgart.iste.cowolf.ui.mainPage"
            name="CoWolf">
      </page>
   </extension>
</plugin>
