<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="statechartModelManager"
         point="de.uni_stuttgart.iste.cowolf.model.architectureModelManagerExtension">
      <architectureModelManager
            class="de.uni_stuttgart.iste.cowolf.model.statechart.StatemachineModelManager">
      </architectureModelManager>
   </extension>


   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated statemachine -->
      <package
            uri="http://Statemachine/1.0"
            class="de.uni_stuttgart.iste.cowolf.model.statechart.StatemachinePackage"
            genModel="model/statemachine.genmodel"/>
   </extension>

</plugin>
